-- Select the database
USE leaselinker;

-- Check existing tables
SHOW TABLES;
-- Insert menus
INSERT INTO menus (mid, icon, menu) VALUES
(1, '/images/list.png', 'Property Posting'),
(2, '/images/logo.png', 'Property Search'),
(3, '/images/profile.png', 'My Profile');

-- Insert roles
INSERT INTO roles (id, role, mid) VALUES
(1, 1, 1),
(2, 1, 2),
(3, 1, 3),
(4, 2, 1),
(5, 2, 2),
(6, 2, 3),
(7, 3, 2),
(8, 3, 3);
INSERT INTO menus (mid, icon, menu) VALUES
(1, '/images/list.png', 'Job Posting'),
(2, '/images/logo.png', 'Job Search'),
(3, '/images/profile.png', 'My Profile');
DELETE FROM roles;   -- remove dependent rows first
DELETE FROM menus;   -- now you can delete menus
